
{
  "family": "ecs-devops-sandbox-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "nodejsapp-container",
      "image": "REPLACE_ME", 
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "memory": 512,
      "cpu": 256
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::652286699990:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::652286699990:role/ecsTaskExecutionRole"
}
